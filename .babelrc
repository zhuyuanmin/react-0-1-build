{
  "presets": [
    ["@babel/preset-react", {"throwIfNamespace": false}],
    [
      "@babel/preset-env",
      {
        "targets": {
          "chrome": "58",
          "ie": "11",
          "esmodules": true
        }
      }
    ]
  ],
  "plugins": ["@babel/plugin-transform-runtime", "@babel/plugin-proposal-class-properties"]
}
